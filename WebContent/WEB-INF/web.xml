<?xml version="1.0" encoding="UTF-8"?>
<web-app>

<servlet>
<servlet-name>UserSignup</servlet-name>
<servlet-class>com.user.UserSignup</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>UserSignup</servlet-name>
<url-pattern>/usersignup</url-pattern>
</servlet-mapping>


<servlet>
<servlet-name>UserLogin</servlet-name>
<servlet-class>com.user.UserLogin</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>UserLogin</servlet-name>
<url-pattern>/userlogin</url-pattern>
</servlet-mapping>


<servlet>
<servlet-name>AdminLogin</servlet-name>
<servlet-class>com.admin.AdminLogin</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>AdminLogin</servlet-name>
<url-pattern>/adminlogin</url-pattern>
</servlet-mapping>


</web-app>